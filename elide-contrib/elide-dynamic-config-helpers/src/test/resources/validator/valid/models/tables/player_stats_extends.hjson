{
  tables:
  [
    {
      name: PlayerExtend
      schema: playerdb
      table: player
      extend: Player
      description: Player Extend
      cardinality: large
      measures : [
            {
            name : highScore
            type : "INTEGER"
            override : true
            definition: '<%measure_type%>(score)'
            },
            {
            name : AvgScore
            type : "INTEGER"
            override : false
            definition: 'AVG(score)'
            }
        ]
    }
  ]
}
