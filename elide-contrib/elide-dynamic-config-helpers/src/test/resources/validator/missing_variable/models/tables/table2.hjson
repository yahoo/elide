{
    // should work fine if missing variables.hjson not provided.
    tables: [{
        name: Player
        table: player
        schema: playerdb
        description:
        '''
        A long description
        '''
        cardinality : large
        readAccess : A user is admin or is a player in the game
        measures : [
            {
            name : highScore
            type : "INTEGER"
            definition: 'MAX({{score}})'
            }
        ]
        dimensions : [
            {
                name : createdOn
                type : TIME
                definition : '{{create_on}}'
                grain:
                {
                    type : DAY
                    sql :  '''
                    PARSEDATETIME(FORMATDATETIME(${column}, 'yyyy-MM-dd'), 'yyyy-MM-dd')
                    '''
                }
            }
        ]
    }]
}
