{
    "$schema": "https://json-schema.org/draft-06/schema#",
    "$id": "https://elide.io/schemas/variable_schema_v1#",
    "description": "Elide Variable config json/hjson schema",
    "type": "object",
    "patternProperties": {
        "^([A-Za-z0-9_]+[.]?)+$": {
          "anyOf": [
            {"type": "string"},
            {"type": "array"},
            {"type": "object"},
            {"type": "null"}
          ]
        }
      },
      "additionalProperties": false,
      "minProperties": 1
}
