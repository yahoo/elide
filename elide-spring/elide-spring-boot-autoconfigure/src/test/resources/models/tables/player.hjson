{
    tables: [{
        name: Player
        table: player
        schema: playerdb
        description:
        '''
        A long description
        '''
        cardinality : large
        readAccess : Prefab.Role.All
        joins: [
            {
                name: playerCountry
                to: string
                type: toOne
                definition: '${to}.id = ${from}.country_id'
            }
        ]
        measures : [
            {
            name : highScore
            type : INTEGER
            definition: 'MAX(score)'
            readAccess : Prefab.Role.All
            }
        ]
        dimensions : [
            {
                name : countryCode
                type : TEXT
                readAccess : Prefab.Role.All
                definition : playerCountry.isoCode
            },
            {
                name : createdOn
                type : TIME
                readAccess : Prefab.Role.All
                definition : create_on
                grains: [
                {
                    grain :  MONTH
                    sql :  '''
                    PARSEDATETIME(FORMATDATETIME(${column}, 'yyyy-MM-01'), 'yyyy-MM-dd')
                    '''
                }
                ]
            }
        ]
    }]
}
